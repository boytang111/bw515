/* pages/home/home.wxss */
@font-face {
    font-family: 'FZShangKuS-R-GB';
    src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'),
        url(data:application/font-woff;charset=utf-8;base64,) format('woff');
    font-weight: normal;
    font-style: normal;
}
page>image:nth-of-type(1){
  position: absolute;
  top: -50rpx;
  width: 750rpx;
  z-index: 0
}
page>image:nth-of-type(2){
  position: absolute;
  z-index: 1;
  width: 82rpx;
  left: 461rpx;
  top: 439rpx;
}
page>image:nth-of-type(3){
  position: absolute;
  z-index: 1;
  width: 88rpx;
  left: 517rpx;
  top: 337rpx;
  animation: fingerMove 1s infinite
}
page>text:nth-of-type(1){
  position: absolute;
  z-index: 1;
  left: 500rpx;
  top: 295rpx;
  font-size: 22rpx;
  color: #ffffff
}
page>image:nth-of-type(4){
  position: absolute;
  z-index: 1;
  width: 724rpx;
  left: 25rpx;
  top: 75rpx;
}
page>image:nth-of-type(5){
  position: absolute;
  z-index: 2;
  width: 115rpx;
  left: 320rpx;
  top: 225rpx;
}
page>image:nth-of-type(6){
  position: absolute;
  z-index: 2;
  width: 217rpx;
  left: 120rpx;
  top: 155rpx;
}
page>view:nth-of-type(1){
  position: fixed;
  height: 100%;
  width: 100%;
  z-index: 3;
  background-color: rgba(0, 0, 0, 0.3)
}
page>view:nth-of-type(1)>image:nth-of-type(1){
  width: 85%;
  position: absolute;
  top: 145rpx;
  left: 5.5%;
  z-index: 4
}
.text{
  display: block;
  position: absolute;
  top: 450rpx;
  width: 85%;
  left: 8%;
  text-align: center;
  z-index: 5;
  font-size: 70rpx;
  color: #0075d0;
  font-family: 'FZShangKuS-R-GB';
}
@keyframes fingerMove{
  0%{
    margin-left: 0;
    margin-top: 0;
  }
  25%{
    margin-left: 4rpx;
    margin-top: -5rpx;
  }
  50%{
    margin-left: 0;
    margin-top: 0;
  }
  75%{
    margin-left: -4rpx;
    margin-top: 5rpx;
  }
  100%{
    margin-left: 0;
    margin-top: 0;
  }
}